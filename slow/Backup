        DirFrom=/Offline/AMSDataDir/DataBase
        DirTo=/usr0/users/ams/AMSDataDir/DataBase
        export DirFrom
        export  DirTo
        /Offline/vdev/slow/backup 1>/offruns/logs/backup.log 2>&1 &
