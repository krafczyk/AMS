#!/bin/tcsh
if ( "$1" != "" )  then
setenv AMSWD $1
endif

if ( "$2" != "" )  then
setenv ADDBUILDNO 1
else
unsetenv ADDBUILDNO
endif

unsetenv ARCH
unsetenv PGTRACK
source /Offline/vdev/install/amsvar.icc64
gmake all
source /Offline/vdev/install/amsvar.gcc64
gmake allp
source /Offline/vdev/install/amsvar.icc32
gmake allold 
setenv PGTRACK 1
source /Offline/vdev/install/amsvar.icc64
gmake all
source /Offline/vdev/install/amsvar.gcc
gmake allp
source /Offline/vdev/install/amsvar.gcc64
gmake allp
source /Offline/vdev/install/amsvar.icc32
gmake allold 
