#!/bin/tcsh
if ( "$1" != "" )  then
setenv AMSWD $1
endif

if ( "$2" != "" )  then
setenv ADDBUILDNO 1
else
unsetenv ADDBUILDNO
endif

setenv PREFIX /Offline/vdev/install
setenv TESTFLAG 


unsetenv MARCH
unsetenv PGTRACK
setenv ECALBDT 1
source $PREFIX/amsvar$TESTFLAG.icc64
source  $PREFIX/g4i$TESTFLAG.icc64
gmake allg4
source  $PREFIX/amsvar$TESTFLAG.gcc64
gmake allp
gmake server
source  $PREFIX/amsvar$TESTFLAG.icc32
gmake allold 
setenv MARCH linuxicc26
gmake -j
gmake producer -i
unsetenv MARCH
source  $PREFIX/amsvar$TESTFLAG.gcc32
source  $PREFIX/g4i$TESTFLAG.gcc32
gmake allg4
gmake server

setenv PGTRACK 1
source  $PREFIX/amsvar$TESTFLAG.icc64
source  $PREFIX/g4i$TESTFLAG.icc64
gmake allg4
echo "here"
source  $PREFIX/amsvar$TESTFLAG.gcc32
source  $PREFIX/g4i$TESTFLAG.gcc32
gmake allg4
source  $PREFIX/amsvar$TESTFLAG.gcc64
source  $PREFIX/g4i$TESTFLAG.gcc64
gmake allp
source $PREFIX/amsvar$TESTFLAG.icc32
gmake allold 
setenv MARCH linuxicc26
gmake -j
gmake producer -i
unsetenv MARCH
setenv PGTRACK 1
setenv G4AMS 1
source $PREFIX/amsvar$TESTFLAG.icc64
source $PREFIX/g4i$TESTFLAG.icc64
gmake  -j
gmake producer -i
unsetenv G4AMS
unsetenv PGTRACK

