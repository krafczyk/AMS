 setenv ROOTSYS /afs/cern.ch/exp/ams/Offline/root/$OS/old
set path=( $path . /usr/sbin $ROOTSYS/bin )
setenv LD_LIBRARY_PATH .:$ROOTSYS/lib
setenv CVSROOT $Offline/CVS
setenv CVSROOT :pserver:amscvs@ams.cern.ch:/amsdat2/SW/cvsroot
setenv AMSDataDir $Offline/AMSDataDir
setenv EDITOR  emacs
setenv CVSEDITOR  emacs
setenv RunsDir $Offline/RunsDir
setenv AMSDataNtuplesDir $Offline/ntuples
setenv AMSMCNtuplesDir   $Offline/amsmcntuplesdir
setenv ProductionLogDirLocal $Offline/logs.local
setenv RunsSummaryFile   $Offline/runs_STS91.log
setenv SlowRateDataDir   $Offline/SlowRateDataDir
setenv AMSGeoDir         $Offline/vdev/display/v2.00
setenv amsed             $Offline/vdev/display/v2.00/$OS/bin/amsed
setenv amsed02           $Offline/vdev/display/ams02/$OS/bin/amsed      
limit core 0
limit data 700000
limit stack 32000

