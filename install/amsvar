setenv ROOTSYS $Offline/root/$OS
set path=( $path . /usr/sbin $ROOTSYS/bin )
setenv LD_LIBRARY_PATH .:$ROOTSYS/lib
setenv CVSROOT $Offline/CVS
setenv AMSDataDir $Offline/AMSDataDir
setenv EDITOR  emacs
setenv CVSEDITOR  emacs
setenv RunsDir $Offline/RunsDir
setenv AMSDataNtuplesDir $Offline/amsdatantuplesdir
setenv AMSMCNtuplesDir   $Offline/amsmcntuplesdir
setenv ProductionLogDirLocal $Offline/logs.local
setenv RunsSummaryFile   $Offline/runs_Aug.log
setenv SlowRateDataDir   $Offline/SlowRateDataDir
setenv AMSGeoDir         $Offline/vdev/display/v2.00
setenv amsed             $Offline/vdev/display/v2.00/$OS/bin/amseda
limit core 0

