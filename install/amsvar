setenv amsed            $Offline/vdev/display/amsed
setenv amsedc           $Offline/vdev/display/amsedc
setenv amsoffmon        $Offline/vdev/online/offmon      
setenv amsoffmonc        $Offline/vdev/online/offmonc      
setenv AMSICC 1
limit core 0
limit data 700000
