$(BIN)rootdict_s.C:    ../include/point.h ../include/linkdefs.h   ../include/root_RVS.h ../include/root.h  ../include/TrElem.h ../include/trfit.h
	@echo "Generating dictionary"
	$(ROOTSYS)/bin/rootcint    -f $(BIN)rootdict_s.C -c ../include/point.h ../include/root_RVS.h  ../include/amschain.h ../include/VCon.h ../include/TrElem.h  ../include/TrdRawHit.h ../include/TrdHSegment.h ../include/TrdHTrack.h ../include/TrdHRecon.h ../include/TrdHCalib.h ../include/TrdHCharge.h ../include/TrPdf.h  ../include/TrdHRecon.h ../include/TrdHCharge.h ../include/trfit.h ../include/pfunction.h ../include/DynAlignment.h ../include/RichTools.h ../include/RichCharge.h ../include/RichConfig.h ../include/Tofrec02_ihep.h  ../include/Tofcharge_ihep.h ../include/GeoMagField.h ../include/GeoMagCoord.h  ../include/GM_SubLibrary.h  ../include/GeoMagTrace.h ../include/EcalChi2CY.h ../include/linkdefs.h 
	@echo "OK"

$(BIN)rootdict_s.o:         $(BIN)rootdict_s.C
	$(CC) -c $(CFLAGS) $(BIN)rootdict_s.C -o $(BIN)rootdict_s.o 
 

depend: 	
	makedepend -f Mopt -- $(DFLAGS) -- $(FILES) -p$(BIN)


        

