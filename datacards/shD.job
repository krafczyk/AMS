TOFMC 1=90.0e-12
TRMC 19=0
TRCL 21=1       
TRFIT 6=1 7=1 8=1 9=1 10=1 11=1 12=1 13=1 14=1 15=1 16=1 17=1 18=1 19=1
C TRFIT 6=1 7=1 8=1 
IOPA 1=0 2='pr.hbk'
RNDM 1241309313 136819389
C      Simul Debug    CpuLim  Read Write Jobname
AMSJOB 1=0   2=0      3=50.   4=0  5=10   6='v203 Protons '
46='CTCVer '
