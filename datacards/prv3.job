[ -z "$UNAME" ]      && UNAME=`(uname) 2>/dev/null`
[ -z "$UNAME" -a -d /sys/node_data ] && UNAME="DomainOS"
[ -z "$UNAME" ]      && (echo could not determine hosttype ; exit)

if [ "$UNAME" = "AIX" ] ; then
   MACHINE="aix"
elif [ "$UNAME" = "OSF1" ] ; then
    MACHINE="osf1"
elif [ "$UNAME" = "HP-UX" ] ; then
    MACHINE="hpux"
elif [ "$UNAME" = "Linux" ] ; then
    MACHINE="linux"
else
echo $UNAME is not supported yet

fi


ORDER=$1
DELTA=$2
UNIFORM=$3
../exe/$MACHINE/gbatch.exe <<!
LIST
KINE 14 
MULS 1 
C MSEL 1=0
LOSS  1
CUTS 1=0.0005 2=0.0005 3=0.001 4=0.001 5=0.001
HADR 1 
TRIG 300
DEBUG -1 10 1
C    tmaxf  smax   fieldm  cconv   maxcal  nitm    xtol iver ialg   steps 
TKFIT 0.2    50.   2.     5.e-4     8  1000     1.e-2   0     1    0.03 
C npars ims 
  5     1   
C          Nparticles 
MCGEN  1=-1. 2=-1. 3=150.  4=1. 5=1. 6=150. 
  7=0. 8=0. 9=-1. 10=0. 11=0.   
 12=-1.  13=10. 14=10. 15=1 18=1 19=102004 20=4
IOPA 1=3 2='prv3t.hbk' 43=1 
45='TriggerLVL1||AMSTRDMCCluster     '
RNDM 191473093 131623257
C      Simul Debug    CpuLim  Read Write Jobname
AMSJOB 1=0   2=0      3=5.   4=0  5=0   46='AMS02' 
L3REC 11=1
TIME 3=10000000
TERM 1=1234567890
C TOFMC 13=1
MISC 4=1 5=0
C TKGEOM 1=0
C G4FF $ORDER $DELTA $UNIFORM
C G4FF 0 
G4FF  3=1 
END

!
