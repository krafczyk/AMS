# V1.25    Sep 12, 1996. ak. cxxfix, etc
# AIX/OSF1 Sep 18, 1996. ak/vs
# Make dependencies from ../include/*.h  Oct 21, 1996. ak.
# V2.01    Apr 10, 1997. new version of CXX, no cxxfix for Alpha anymore
# V2.02    May   , 1997. lms are replaced by dbA
# V2.10    Feb  7, 1998. jobDB, eventDB, timeidDB
#
# Last Edit : Feb 07, 1998. ak
#
#

#
SHELL=/bin/sh
#
ifeq  "$(shell uname)"  "AIX"
BIN_DB          = ../db/bin/aix/
BIN             = ../bin/aix
EXE             = ../exe/aix/

CFLAGS= -D__AMSDEBUG__  -D__BATCH__ -D__DB__ 
CCFLAGS = -g
FFLAGS=   -g -I../include -qextname -d
DFLAGS=  -D__AMSDEBUG__  -g   -I/usr/include -I/usr/lpp/xlC/include -I../include -I../include/stl
GLIBS= -g -bloadmap:map -lxlf90 -lxlf -L/cern/old/lib  -lgeant321 -lmathlib\
        -lpacklib -lkernlib  -L/afs/cern.ch/asis/packages/licensed/nag-mark16/rs_aix32/nagfl16df -lnag


CC=xlC
FF=xlf
LD=xlC
TIMEL=../bin/aix/timel.o

include config.user_ibmrs6000

INC_OODDL      = /usr/lpp/xlC/include

endif

ifeq  "$(shell uname)"  "OSF1"
BIN_DB          = ../db/bin/osf1/
BIN             = ../bin/osf1
EXE             = ../exe/osf1/

CFLAGS= -D__AMSDEBUG__  -D__BATCH__ -D__ALPHA__ -D__APOOL__ -D__DB__ 
CCFLAGS = -define_templates -g

FFLAGS=   -g -I../include  -d -static -nofor_main -convert big_endian
DFLAGS=  -D__AMSDEBUG__  -g   -I/usr/include  -I../include -I../include/stl \
         -I/usr/include/cxx

GLIBS = -g -lUfor   -lfor -lFutil -lots -lm -L/cern/old/lib  -lgeant321 -lmathlib -lpacklib -lkernlib  -L/afs/cern.ch/asis/packages/licensed/nag-mark16/alpha_osf32c/nagfl16df -lnag

CC=cxx
FF=f77
LD=cxx
TIMEL=../bin/osf1/timel.o

include config.user_alphaosf1

endif


#########################
#                       #
# Configuration Section #
#                       #
#########################

BOOTF=/offdata/database/boot/LOM
BOOT =/offdata/database/boot/LOM
#BOOT=/dbase/newV/AMS/db/boot/LOM
#include config.user
include config.$(ARCH)
#########################
#                       #
# Program Executables   #
#                       #
#########################

LMS	= $(EXE)lmsS

#########################
#                       #
# Schema Files          #
#                       #
#########################

DDL_SRC         = ../db/

SCHEMA_DDL		=  dbcatalog.ddl \
                           gvolumeD.ddl  gmatD.ddl  gtmedD.ddl \
                           commonsD.ddl amsdbcD.ddl antidbcD.ddl \
                           ctcdbcD.ddl tofdbcD.ddl tkdbcV.ddl \
                           timeidD.ddl 


SCHEMA_SRCS		= $(SCHEMA_DDL:.ddl=_ddl.C)
#SCHEMA_OBJS		= $(SCHEMA_DDL:.ddl=_ddl.o)
SCHEMA_METHOD_SRCS	= $(SCHEMA_DDL:.ddl=.C)
#SCHEMA_METHOD_OBJS	= $(SCHEMA_DDL:.ddl=.o)
SCHEMA_HDRS_REF		= $(SCHEMA_DDL:.ddl=_ref.h)
SCHEMA_HDRS_NOREF	= $(SCHEMA_DDL:.ddl=.h)
SCHEMA_HDRS		= $(SCHEMA_HDRS_REF) $(SCHEMA_HDRS_NOREF)

SCHEMA_OBJS		=   $(BIN_DB)dbcatalog_ddl.o  \
                            $(BIN_DB)gvolumeD_ddl.o \
                            $(BIN_DB)gmatD_ddl.o  \
                            $(BIN_DB)gtmedD_ddl.o \
                            $(BIN_DB)commonsD_ddl.o \
                            $(BIN_DB)amsdbcD_ddl.o \
                            $(BIN_DB)antidbcD_ddl.o \
                            $(BIN_DB)ctcdbcD_ddl.o \
                            $(BIN_DB)tofdbcD_ddl.o \
                            $(BIN_DB)tkdbcV_ddl.o \
                            $(BIN_DB)timeidD_ddl.o 



SCHEMA_METHOD_OBJS      =  $(BIN_DB)dbcatalog.o  \
                           $(BIN_DB)gvolumeD.o \
                           $(BIN_DB)gmatD.o  \
                           $(BIN_DB)gtmedD.o \
                           $(BIN_DB)commonsD.o \
                           $(BIN_DB)amsdbcD.o \
                           $(BIN_DB)antidbcD.o \
                           $(BIN_DB)ctcdbcD.o \
                           $(BIN_DB)tofdbcD.o \
                           $(BIN_DB)tkdbcV.o \
                           $(BIN_DB)timeidD.o 


#########################
#                       #
# Program Source        #
#                       #
#########################


LMS_SRCS	= dbS.C   \
		  dbS_Init.C \
		  dbS_Setup.C \
		  dbS_Sub.C   \
		  dbS_Delete.C \
                  dbS_TDV.C     \
                  ooSession.C     \
                  ooStats.C        \
                  ooEnvironment.C   \
                  ../CC/geant.C      \
                  ../CC/job.C         \
                  ../CC/timeid.C

LMS_OBJS	= $(BIN_DB)dbS.o   \
		  $(BIN_DB)dbS_Init.o \
		  $(BIN_DB)dbS_Setup.o \
		  $(BIN_DB)dbS_Sub.o \
		  $(BIN_DB)dbS_Delete.o \
                  $(BIN_DB)dbS_TDV.o \
                  $(BIN_DB)ooSession.o     \
                  $(BIN_DB)ooStats.o        \
                  $(BIN_DB)ooEnvironment.o   \
                  $(BIN_DB)geantDB.o         \
                  $(BIN_DB)jobDB.o           \
                  $(BIN_DB)eventDB.o          \
                  $(BIN_DB)timeidDB.o          

#########################
#                       #
# Program Directories   #
#                       #
#########################

INC_DIR		= $(INSTALL_DIR)/$(ARCH)/include
LIB_DIR		= $(INSTALL_DIR)/$(ARCH)/lib
BIN_DIR		= $(INSTALL_DIR)/$(ARCH)/bin

INC_AMS         = ../include
INC_STL         = ../include/stl
INC_DDL         = ../db 

#########################
#                       #
# Objectivity Tools	#
#                       #
#########################

OODDL		= $(BIN_DIR)/ooddlx
OONEWFD		= $(BIN_DIR)/oonewfd
OODELETEFD	= $(BIN_DIR)/oodeletefd

#########################
#                       #
# Compile Rules         #
#                       #
#########################

INCLUDES = -I$(INC_DIR) -I$(INC_DDL) -I$(INC_AMS) -I$(INC_STL)

.SUFFIXES: .C .ddl 

#.C.o:
#	$(C++) $(C_FLAGS) $(CFLAGS) $(INCLUDES) -c $*.C -o $(BIN_DB)$*.o


#########################
#                       #
# Object Files          #
#                       #
#########################

DB_OBJS	= $(SCHEMA_OBJS) $(SCHEMA_METHOD_OBJS) $(LMS_OBJS)

#########################
#                       #
# Link Rules            #
#                       #
#########################

OBJECT_LIBS	= $(LIB_DIR)/liboo.a
SRC             = ../CC

GEANT_OBJS= $(BIN)/gbatch.o $(BIN)/commons.o $(BIN)/gvolume.o  $(BIN)/id.o $(BIN)/node.o $(BIN)/snode.o $(BIN)/cern.o $(BIN)/amsdbc.o $(BIN)/gsen.o  $(BIN)/mtx.o $(BIN)/point.o $(BIN)/amstkshc++.o $(BIN)/sgif.o $(BIN)/upool.o $(BIN)/gmat.o $(BIN)/amsgobj.o $(BIN)/amsgeom.o  $(BIN)/mceventg.o $(BIN)/tkmccl.o $(BIN)/rnormx.o  $(BIN)/trid.o $(BIN)/link.o $(BIN)/trrec.o $(BIN)/uzstat.o $(BIN)/tptcir.o $(BIN)/trafit.o $(BIN)/tcircf.o $(BIN)/tszfit.o $(BIN)/txyfit.o  $(BIN)/cont.o $(BIN)/trrawcluster.o $(BIN)/mccluster.o $(BIN)/rndm.o $(BIN)/tofrec.o $(BIN)/antirec.o $(BIN)/tofsim.o $(BIN)/beta.o $(BIN)/charge.o $(BIN)/particle.o $(BIN)/apool.o $(BIN)/tofdbc.o $(BIN)/ctcrec.o $(BIN)/ctcsim.o $(BIN)/ctcdbc.o $(BIN)/tofcalib.o $(BIN)/io.o $(BIN)/trcalib.o $(BIN)/mtx2.o $(BIN)/e04ccf.o $(BIN)/trigger1.o $(BIN)/trigger3.o $(BIN)/antidbc.o $(BIN)/daqevt.o $(BIN)/daqblock.o $(BIN)/astring.o $(BIN)/mfield.o $(BIN)/egzav.o $(BIN)/tkdbc.o $(BIN)/user.o $(BIN)/tofonline.o $(BIN)/ntuple.o



CERN_LIBS       = -L/cern/pro/lib  -lgeant321 -lmathlib -lpacklib -lkernlib

all:	$(LMS)

$(LMS): $(DB_OBJS) 
	$(LD) $(LD_FLAGS) -o $(LMS) $(DB_OBJS) $(GEANT_OBJS) $(OBJECT_LIBS) \
        $(GLIBS)

#########################
#                       #
# Schema Rules          #
#                       #
#########################

$(BOOT):
	$(OONEWFD) -fdlocalpath $(BOOTF).FDDB -pagesize $(PAGESIZE) -lockserverhost $(LS_HOST)  -fdnumber $(FDID) $(BOOT)

.ddl.h: 
	$(OODDL) -D__DECCXX $(INCLUDES) $*.ddl $(BOOT)

$(SCHEMA_DDL:.ddl=.h): $(BOOT)

#########################
#                       #
# Other Rules           #
#                       #
#########################

clean:
	rm -rf $(LMS) $(DB_OBJS) $(SCHEMA_SRCS) $(SCHEMA_HDRS) Templates.DB
	$(OODELETEFD) -force $(BOOT)

#########################
#                       #
# Dependencies		#
#                       #
#########################

 $(BIN_DB)geantDB.o:         ../CC/geant.C dbS.h
	$(C++) $(C_FLAGS) $(CFLAGS) $(INCLUDES) $(CCFLAGS) -c ../CC/geant.C \
                                                -o $(BIN_DB)geantDB.o
 $(BIN_DB)jobDB.o:         ../CC/job.C dbS.h
	$(C++) $(C_FLAGS) $(CFLAGS) $(INCLUDES) $(CCFLAGS) -c ../CC/job.C \
                                                -o $(BIN_DB)jobDB.o
 $(BIN_DB)eventDB.o:         ../CC/event.C dbS.h
	$(C++) $(C_FLAGS) $(CFLAGS) $(INCLUDES) $(CCFLAGS) -c ../CC/event.C \
                                                -o $(BIN_DB)eventDB.o
