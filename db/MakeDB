#########################
#                       #
# Program Directories   #
#                       #
#########################

INC_DIR		= $(INSTALL_DIR)/$(ARCH)/include
LIB_DIR		= $(INSTALL_DIR)/$(ARCH)/lib
BIN_DIR		= $(INSTALL_DIR)/$(ARCH)/bin

INC_AMS         = ../include
INC_STL         = ../include/stl
INC_DDL         = ../db 

#########################
#                       #
# Objectivity Tools	#
#                       #
#########################

OODDL		= $(BIN_DIR)/ooddlx
OONEWFD		= $(BIN_DIR)/oonewfd
OODELETEFD	= $(BIN_DIR)/oodeletefd

#########################
#                       #
# Compile Rules         #
#                       #
#########################

INCLUDES = -I$(INC_DIR) -I$(INC_DDL) -I$(INC_AMS) -I$(INC_STL)

.SUFFIXES: .C .ddl 

#.C.o:
#	$(C++) $(C_FLAGS) $(CFLAGS) $(INCLUDES) -c $*.C -o $(BIN_DB)$*.o


#########################
#                       #
# Object Files          #
#                       #
#########################

DB_OBJS	= $(SCHEMA_OBJS) $(SCHEMA_METHOD_OBJS) $(LMS_OBJS)

#########################
#                       #
# Link Rules            #
#                       #
#########################

OBJECT_LIBS	= $(LIB_DIR)/liboo.a
SRC             = ../CC

GEANT_OBJS= $(BIN)/gbatch.o $(BIN)/commons.o $(BIN)/gvolume.o  $(BIN)/id.o $(BIN)/node.o $(BIN)/snode.o $(BIN)/cern.o $(BIN)/amsdbc.o $(BIN)/gsen.o  $(BIN)/mtx.o $(BIN)/point.o $(BIN)/amstkshc++.o $(BIN)/sgif.o $(BIN)/upool.o $(BIN)/gmat.o $(BIN)/amsgobj.o $(BIN)/amsgeom.o $(BIN)/mceventg.o $(BIN)/tkmccl.o $(BIN)/rnormx.o  $(BIN)/trid.o $(BIN)/link.o $(BIN)/trrec.o $(BIN)/uzstat.o $(BIN)/tptcir.o $(BIN)/trafit.o $(BIN)/tcircf.o $(BIN)/tszfit.o $(BIN)/txyfit.o  $(BIN)/cont.o $(BIN)/trrawcluster.o $(BIN)/mccluster.o $(BIN)/rndm.o $(BIN)/tofrec.o $(BIN)/tofonline.o $(BIN)/antirec.o $(BIN)/tofsim.o $(BIN)/beta.o $(BIN)/charge.o $(BIN)/particle.o $(BIN)/apool.o $(BIN)/tofdbc.o $(BIN)/ctcrec.o $(BIN)/ctcdbc.o $(BIN)/tofcalib.o $(BIN)/timeid.o $(BIN)/io.o $(BIN)/trcalib.o $(BIN)/mtx2.o $(BIN)/e04ccf.o $(BIN)/trigger1.o $(BIN)/trigger3.o $(BIN)/daqevt.o $(BIN)/antidbc.o $(BIN)/daqblock.o $(BIN)/astring.o $(BIN)/ins_dummy.o  $(BIN)/mfield.o $(BIN)/egzav.o $(BIN)/tkdbc.o $(BIN)/ctcsim.o $(BIN)/user.o

CERN_LIBS       = -L/cern/pro/lib  -lgeant321 -lmathlib -lpacklib -lkernlib

all:	$(LMS)

$(LMS): $(DB_OBJS) $(GEANT_OBJS)
	$(LD) $(LD_FLAGS) -o $(LMS) $(DB_OBJS) $(GEANT_OBJS) $(OBJECT_LIBS) \
        $(GLIBS)

#########################
#                       #
# Schema Rules          #
#                       #
#########################

$(BOOT): config.user_alphaosf1
	$(OONEWFD) -fdlocalpath $(BOOTF).FDDB -pagesize $(PAGESIZE) -lockserverhost $(LS_HOST)  -fdnumber $(FDID) $(BOOT)

.ddl.h: 
	$(OODDL) -D__DECCXX $(INCLUDES) $*.ddl $(BOOT)

$(SCHEMA_DDL:.ddl=.h): $(BOOT)

#########################
#                       #
# Other Rules           #
#                       #
#########################

clean:
	rm -rf $(LMS) $(DB_OBJS) $(SCHEMA_SRCS) $(SCHEMA_HDRS) Templates.DB
	$(OODELETEFD) -force $(BOOT)

