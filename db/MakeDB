		  $(BIN_DB)A_LMSAdd.o \
		  $(BIN_DB)A_LMSFill.o \
		  $(BIN_DB)A_LMSMenu.o \
		  $(BIN_DB)A_LMSDelete.o \
		  $(BIN_DB)A_LMSPrint.o \
                  $(BIN_DB)ooSession.o     \
                  $(BIN_DB)ooStats.o        \
                  $(BIN_DB)geantDB.o
