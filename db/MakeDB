ifeq  "$(shell uname)"  "OSF1"

LMS_SRCS	= A_LMS.C   \
		  A_LMSAdd.C \
		  A_LMSFill.C \
		  A_LMSMenu.C \
		  A_LMSDelete.C \
		  A_LMSPrint.C \
                  ooSession.C     \
                  ooStats.C        \
                  cxxfix.C          

